								<TextBlock Grid.Column="3" Grid.Row="4" Text="{Binding Path=ReportText.BtZ}" HorizontalAlignment="Center" Foreground="White" Margin="5"  /-->
                                <TextBlock Grid.Column="4" Grid.Row="4" Text="{Binding Path=ReportText.BtInstrumentMag}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="5" Grid.Row="4" Text="{Binding Path=ReportText.BtInstrumentDir}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="6" Grid.Row="4" Text="{Binding Path=ReportText.BtInstrumentPercentError}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="7" Grid.Row="4" Text="{Binding Path=ReportText.BtInstrumentDirError}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />

                                <!--Water Profile Earth-->
                                <!--TextBlock Grid.Column="1" Grid.Row="5" Text="{Binding Path=ReportText.WpE}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
								<TextBlock Grid.Column="2" Grid.Row="5" Text="{Binding Path=ReportText.WpN}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
								<TextBlock Grid.Column="3" Grid.Row="5" Text="{Binding Path=ReportText.WpU}" HorizontalAlignment="Center" Foreground="White" Margin="5"  /-->
                                <TextBlock Grid.Column="4" Grid.Row="5" Text="{Binding Path=ReportText.WpEarthMag}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="5" Grid.Row="5" Text="{Binding Path=ReportText.WpEarthDir}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="6" Grid.Row="5" Text="{Binding Path=ReportText.WpEarthPercentError}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="7" Grid.Row="5" Text="{Binding Path=ReportText.WpEarthDirError}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />

                                <!--Water Profile Instrument-->
                                <!--TextBlock Grid.Column="1" Grid.Row="6" Text="{Binding Path=ReportText.WpX}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
								<TextBlock Grid.Column="2" Grid.Row="6" Text="{Binding Path=ReportText.WpY}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
								<TextBlock Grid.Column="3" Grid.Row="6" Text="{Binding Path=ReportText.WpZ}" HorizontalAlignment="Center" Foreground="White" Margin="5"  /-->
                                <TextBlock Grid.Column="4" Grid.Row="6" Text="{Binding Path=ReportText.WpInstrumentMag}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="5" Grid.Row="6" Text="{Binding Path=ReportText.WpInstrumentDir}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="6" Grid.Row="6" Text="{Binding Path=ReportText.WpInstrumentPercentError}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                                <TextBlock Grid.Column="7" Grid.Row="6" Text="{Binding Path=ReportText.WpInstrumentDirError}" HorizontalAlignment="Center" Foreground="White" Margin="5"  />
                            </Grid>
                        </Viewbox>
                    </Border>
                    <!--DMG-->

                    <!--Profile Range-->
                    <Grid Grid.Row="1" Margin="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <Border Grid.RowSpan="3" Grid.ColumnSpan="4" Background="{StaticResource PulseBackBorder1Color}" CornerRadius="5" />
                        <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4" Foreground="{StaticResource PulseFontColor}" Text="Profile Range" HorizontalAlignment="Center" />

                        <!--Beam 0-->
                        <Border Grid.Row="1" Grid.Column="0" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5" Margin="5">
                            <TextBlock Text="B0" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />
                        </Border>
                        <TextBlock Grid.Row="2" Grid.Column="0" Text="{Binding Path=ReportText.ProfileRangeBeam0Str}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                        <!--Beam 1-->
                        <Border Grid.Row="1" Grid.Column="1" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5"  Margin="5">
                            <TextBlock Text="B1" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center"  Margin="5,0" />
                        </Border>
                        <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding Path=ReportText.ProfileRangeBeam1Str}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                        <!--Beam 2-->
                        <Border Grid.Row="1" Grid.Column="2" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5"  Margin="5">
                            <TextBlock Text="B2" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center"  Margin="5,0" />
                        </Border>
                        <TextBlock Grid.Row="2" Grid.Column="2" Text="{Binding Path=ReportText.ProfileRangeBeam2Str}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                        <!--Beam 3-->
                        <Border Grid.Row="1" Grid.Column="3" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5"  Margin="5">
                            <TextBlock Text="B3" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center"  Margin="5,0" />
                        </Border>
                        <TextBlock Grid.Row="2" Grid.Column="3" Text="{Binding Path=ReportText.ProfileRangeBeam3Str}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                    </Grid>
                    <!--Profile Range-->

                    <!--Good Ensembles and Bad Status-->
                    <StackPanel Grid.Row="2" Orientation="Horizontal">

                        <!--Ensembles Good-->
                        <Viewbox Margin="5">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <Border Grid.RowSpan="3" Grid.ColumnSpan="3" Background="{StaticResource PulseBackBorder1Color}" CornerRadius="5" />
                                <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Foreground="{StaticResource PulseFontColor}" Text="Good Ensembles" HorizontalAlignment="Center" />

                                <Border Grid.Row="1" Grid.Column="0" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5" Margin="5">
                                    <TextBlock Grid.Row="1" Grid.Column="0" Text="Good" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />
                                </Border>
                                <TextBlock Grid.Row="2" Grid.Column="0" Text="{Binding Path=GoodEnsembleCount}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                                <Border Grid.Row="1" Grid.Column="1" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5"  Margin="5">
                                    <TextBlock Grid.Row="1" Grid.Column="1" Text="Total" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center"  Margin="5,0" />
                                </Border>
                                <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding Path=EnsembleCount}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                                <Border Grid.Row="1" Grid.Column="2" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5"  Margin="5">
                                    <TextBlock Grid.Row="1" Grid.Column="2" Text="%" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center"  Margin="5,0" />
                                </Border>
                                <TextBlock Grid.Row="2" Grid.Column="2" Text="{Binding Path=GoodEnsemblePercentage}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                            </Grid>
                        </Viewbox>
                        <!--Good Ensemble-->

                        <!--Bad Status-->
                        <Viewbox Margin="5">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <Border Grid.RowSpan="3" Grid.ColumnSpan="3" Background="{StaticResource PulseBackBorder1Color}" CornerRadius="5" />
                                <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Foreground="{StaticResource PulseFontColor}" Text="Bad Status" HorizontalAlignment="Center" />

                                <Border Grid.Row="1" Grid.Column="0" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5" Margin="5">
                                    <TextBlock Grid.Row="1" Grid.Column="0" Text="Bad" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />
                                </Border>
                                <Border Grid.Row="2" Grid.Column="0" Background="{StaticResource PulseStatusColorBad}" CornerRadius="5" Visibility="{Binding Path=IsBadStatusCount, Converter={StaticResource BoolToVisConverter}}" />
                                <TextBlock Grid.Row="2" Grid.Column="0" Text="{Binding Path=BadStatusCount}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                                <Border Grid.Row="1" Grid.Column="1" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5"  Margin="5">
                                    <TextBlock Grid.Row="1" Grid.Column="1" Text="Total" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center"  Margin="5,0" />
                                </Border>
                                <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding Path=EnsembleCount}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />

                                <Border Grid.Row="1" Grid.Column="2" Background="{StaticResource PulseBackBorder2Color}" CornerRadius="5"  Margin="5">
                                    <TextBlock Grid.Row="1" Grid.Column="2" Text="%" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center"  Margin="5,0" />
                                </Border>
                                <TextBlock Grid.Row="2" Grid.Column="2" Text="{Binding Path=BadStatusPercentage}" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" Margin="5,0" />
                            </Grid>
                        </Viewbox>
                    </StackPanel>

                    

                </Grid>
            </Viewbox>

            <!--Settings-->
			<!--Must be below the plot to be on top of the plot-->
			<!--Use a canvas with an expander inside so the plot below can be selected by the mouse-->
			<!--http://karlshifflett.wordpress.com/2008/02/05/wpf-sample-series-expander-control-with-popup-content/-->
            <Canvas Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="0" Panel.ZIndex="99" MinHeight="25">
                <Expander Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="0" Header="Settings" Foreground="{StaticResource PulseFontColor}" >
                    <Border Background="{StaticResource PulseSettingsBackground}" Margin="5" CornerRadius="5">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>

                            <!--Border Grid.Column="0" Grid.ColumnSpan="7" Grid.Row="0" Background="{StaticResource PulseSettingsBackBorder1Color}" CornerRadius="5" /-->

                            <!-- Plot Colors -->
                            <Border Grid.Column="0" Grid.Row="0" BorderBrush="{StaticResource PulseSettingsBackBorderBrush}" BorderThickness="1" CornerRadius="5" Margin="5">
                                <Grid Margin="5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Margin="5">

                                        <!--Border Grid.Row="0" Grid.ColumnSpan="5" CornerRadius="10,10,0,0" BorderBrush="#FF372B2B" BorderThickness="0">
										<Border.Background>
											<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
												<GradientStop Color="Black" Offset="0.969"/>
												<GradientStop Color="#FF373636"/>
											</LinearGradientBrush>
										</Border.Background>
										<TextBlock Text="Plot Colors" Foreground="#FFC5CFD1" HorizontalAlignment="Center" />
									</Border-->

                                        <Border Grid.Row="0" Grid.ColumnSpan="5" Background="{StaticResource PulseSettingsBackBorder3Color}" CornerRadius="5" >
                                            <TextBlock Text="Plot Colors" Foreground="{StaticResource PulseFontColor}" FontWeight="Bold" HorizontalAlignment="Center" Margin="5" />
                                        </Border>

                                    </Grid>

                                    <Grid Grid.Row="1" Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>

                                        <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="4" Background="{StaticResource PulseSettingsBackBorder1Color}" CornerRadius="3" />

                                        <!-- Beam 1 Color -->
                                        <TextBlock Grid.Column="0" Grid.Row="0" Text="Beam 0 Color:" Foreground="{StaticResource PulseFontColor}" Margin="5" HorizontalAlignment="Right" />
                                        <ComboBox Grid.Column="1" Grid.Row="0" ItemsSource="{Binding BeamColorList}" SelectedItem="{Binding Beam0Color}" ItemTemplate="{StaticResource OxyColorComboBox}" Margin="5" />

                                        <!-- Beam 2 Color -->
                                        <TextBlock Grid.Column="0" Grid.Row="1" Text="Beam 1 Color:" Foreground="{StaticResource PulseFontColor}" Margin="5" HorizontalAlignment="Right" />
                                        <ComboBox Grid.Column="1" Grid.Row="1" ItemsSource="{Binding BeamColorList}" SelectedItem="{Binding Beam1Color}" ItemTemplate="{StaticResource OxyColorComboBox}" Margin="5" />


                                        <!-- Beam 3 Color -->
                                        <TextBlock Grid.Column="0" Grid.Row="2" Text="Beam 2 Color:" Foreground="{StaticResource PulseFontColor}" Margin="5" HorizontalAlignment="Right" />
                                        <ComboBox Grid.Column="1" Grid.Row="2" ItemsSource="{Binding BeamColorList}" SelectedItem="{Binding Beam2Color}" ItemTemplate="{StaticResource OxyColorComboBox}" Margin="5" />

                                        <!-- Beam 4 Color -->
                                        <TextBlock Grid.Column="0" Grid.Row="3" Text="Beam 3 Color:" Foreground="{StaticResource PulseFontColor}" Margin="5" HorizontalAlignment="Right" />
                                        <ComboBox Grid.Column="1" Grid.Row="3" ItemsSource="{Binding BeamColorList}" SelectedItem="{Binding Beam3Color}" ItemTemplate="{StaticResource OxyColorComboBox}" Margin="5" />

                                    </Grid>
                                </Grid>
                            </Border>
                            <!-- Plot Colors -->

                            <!--Recording Options-->
                            <Border Grid.Column="1" Grid.Row="0" BorderBrush="{StaticResource PulseSettingsBackBorderBrush}" BorderThickness="1" CornerRadius="5" Margin="5">
                                <Grid Margin="5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Margin="5">

                                        <!--Border Grid.Row="0" Grid.ColumnSpan="5" CornerRadius="10,10,0,0" BorderBrush="#FF372B2B" BorderThickness="0">
										<Border.Background>
											<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
												<GradientStop Color="Black" Offset="0.969"/>
												<GradientStop Color="#FF373636"/>
											</LinearGradientBrush>
										</Border.Background>
										<TextBlock Text="Recording Options" Foreground="#FFC5CFD1" HorizontalAlignment="Center" />
									</Border-->

                                        <Border Grid.Row="0" Grid.ColumnSpan="5" Background="{StaticResource PulseSettingsBackBorder3Color}" CornerRadius="5">
                                            <TextBlock Text="Recording Options" Foreground="{StaticResource PulseFontColor}" FontWeight="Bold" HorizontalAlignment="Center" Margin="5" />
                                        </Border>


                                    </Grid>

                                    <Grid Grid.Row="1" Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>

                                        <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Background="{StaticResource PulseSettingsBackBorder1Color}" CornerRadius="3" />

                                        <!-- Directory Path -->
                                        <TextBlock Grid.Column="0" Grid.Row="0" Text="Directory:" Foreground="{StaticResource PulseFontColor}" Margin="5" HorizontalAlignment="Right" />
                                        <TextBox Grid.Column="1" Grid.Row="0" Width="300" Text="{Binding Path=LakeTestDirectory}"/>

                                        <!--Create Project-->
                                        <TextBlock Grid.Column="0" Grid.Row="1" Text="Create Project:" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="5" />
                                        <CheckBox Grid.Column="1" Grid.Row="1" IsChecked="{Binding IsCreatingProject}" Margin="5" Style="{StaticResource OnOffSwitchStyle}" HorizontalAlignment="Left" />

                                        <!--Record Test Result-->
                                        <TextBlock Grid.Column="0" Grid.Row="2" Text="Record Test Results:" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="5" />
                                        <CheckBox Grid.Column="1" Grid.Row="2" IsChecked="{Binding IsRecordTestResults}" Margin="5" Style="{StaticResource OnOffSwitchStyle}" HorizontalAlignment="Left" />

                                    </Grid>
                                </Grid>
                            </Border>
                            <!--Directory-->

                            <!--Max Ensembles-->
                            <Border Grid.Column="2" Grid.Row="0" BorderBrush="{StaticResource PulseSettingsBackBorderBrush}" BorderThickness="1" CornerRadius="5" Margin="5">
                                <Grid Margin="5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Margin="5">

                                        <!--Border Grid.Row="0" Grid.ColumnSpan="5" CornerRadius="10,10,0,0" BorderBrush="#FF372B2B" BorderThickness="0">
										<Border.Background>
											<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
												<GradientStop Color="Black" Offset="0.969"/>
												<GradientStop Color="#FF373636"/>
											</LinearGradientBrush>
										</Border.Background>
										<TextBlock Text="Max Ensembles" Foreground="#FFC5CFD1" HorizontalAlignment="Center" />
									</Border-->

                                        <Border Grid.Row="0" Grid.ColumnSpan="5" Background="{StaticResource PulseSettingsBackBorder3Color}" CornerRadius="5">
                                            <TextBlock Text="Max Ensembles" Foreground="{StaticResource PulseFontColor}" FontWeight="Bold" HorizontalAlignment="Center" Margin="5" />
                                        </Border>

                                    </Grid>

                                    <Grid Grid.Row="1" Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>

                                        <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="1" Background="{StaticResource PulseSettingsBackBorder1Color}" CornerRadius="3" />

                                        <!--Number of bins to display-->
                                        <TextBlock Grid.Column="0" Grid.Row="0" Text="Max Display Ensembles:" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5" />
                                        <extToolkit:IntegerUpDown Grid.Column="1" Grid.Row="0" Value="{Binding MaxEnsembles}" Minimum="0" HorizontalAlignment="Center" Width="100" Increment="1" Margin="5" />

                                    </Grid>
                                </Grid>
                            </Border>
                            <!--Max Ensembles-->

                            <!--Water Profile Bin-->
                            <Border Grid.Column="3" Grid.Row="0" BorderBrush="{StaticResource PulseSettingsBackBorderBrush}" BorderThickness="1" CornerRadius="5" Margin="5">
                                <Grid Margin="5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Margin="5">

                                        <!--Border Grid.Row="0" Grid.ColumnSpan="5" CornerRadius="10,10,0,0" BorderBrush="#FF372B2B" BorderThickness="0">
										<Border.Background>
											<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
												<GradientStop Color="Black" Offset="0.969"/>
												<GradientStop Color="#FF373636"/>
											</LinearGradientBrush>
										</Border.Background>
										<TextBlock Text="Water Profile Bin" Foreground="#FFC5CFD1" HorizontalAlignment="Center" />
									</Border-->

                                        <Border Grid.Row="0" Grid.ColumnSpan="5" Background="{StaticResource PulseSettingsBackBorder3Color}" CornerRadius="5">
                                            <TextBlock Text="Water Profile Bin" Foreground="{StaticResource PulseFontColor}" FontWeight="Bold" HorizontalAlignment="Center" Margin="5" />
                                        </Border>

                                    </Grid>

                                    <Grid Grid.Row="1" Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>

                                        <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="1" Background="{StaticResource PulseSettingsBackBorder1Color}" CornerRadius="3" />

                                        <!--Number of bins to display-->
                                        <TextBlock Grid.Column="0" Grid.Row="0" Text="Water Profile Bin:" Foreground="{StaticResource PulseFontColor}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5" />
                                        <extToolkit:IntegerUpDown Grid.Column="1" Grid.Row="0" Value="{Binding ReportText.NavBin}" Minimum="0" Maximum="200" HorizontalAlignment="Center" Width="100" Increment="1" Margin="5" />

                                    </Grid>
                                </Grid>
                            </Border>
                            <!--Water Profile Bin-->

                        </Grid>
                    </Border>
                </Expander>
            </Canvas>
			
	    </Grid>
    </ScrollViewer>
</UserControl>
